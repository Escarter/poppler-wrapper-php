{
    "name": "escarter/poppler-wrapper-php",
    "description": "This is a php wrapper for Poppler Utility tools",
    "keywords": [
        "escarter",
        "poppler-wrapper-php"
    ],
    "homepage": "https://github.com/escarter/poppler-wrapper-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Mbutuh Escarter Brieno",
            "email": "mbutuhescarter@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/process": "^5.4|^6.3|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^10.0",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Escarter\\PopplerPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Escarter\\PopplerPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
